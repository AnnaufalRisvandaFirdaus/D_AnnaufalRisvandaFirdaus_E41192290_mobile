<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arf.Sqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="CrudSqlite"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLite">
        <activity android:name="com.arf.Sqlite.UpdateBiodata"
            android:label="Update Biodata">
        </activity>
        <activity android:name="com.arf.Sqlite.LihatBiodata"
            android:label="Lihat Biodata" >
        </activity>
        <activity android:name="com.arf.Sqlite.BuatBiodata"
            android:label="Form Biodata Baru" >
        </activity>
        <activity android:name="com.arf.Sqlite.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>